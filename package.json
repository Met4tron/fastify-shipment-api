    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "fastify": "^4.25.1",
    "fastify-graceful-shutdown": "^3.5.1",
    "kysely": "^0.26.3",
